<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}2sn - SuperSocialNetwork{% endblock %}</title>
        {% block stylesheets %}
	<link href="{{ asset('bundles/ssn/css/reset.css') }}" type="text/css" rel="stylesheet" />
	<link href="{{ asset('bundles/ssn/css/screen.css') }}" type="text/css" rel="stylesheet" />
	<link href="{{ asset('bundles/ssn/css/fancybox.css') }}" type="text/css" rel="stylesheet" />
	<link href="{{ asset('bundles/ssn/css/jquery.wysiwyg.css') }}" type="text/css" rel="stylesheet" />
	<link href="{{ asset('bundles/ssn/css/jquery.ui.css') }}" type="text/css" rel="stylesheet" />
	<link href="{{ asset('bundles/ssn/css/visualize.css') }}" type="text/css" rel="stylesheet" />
	<link href="{{ asset('bundles/ssn/css/visualize-light.css') }}" type="text/css" rel="stylesheet" />
	{% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
      <div class="pagetop">
	<div class="head pagesize">
	  <div class="head_top">
	    <div class="topbuts">
	      <ul class="clear">
		<li><a href="{{ path('timeline') }}">Timeline</a></li>
		<li><a href="{{ path('_settings') }}">Messages</a></li>
		<li><a href="{{ path('_message') }}">Settings</a></li>
                <li><a href="{{ path('logout') }}" class="red">Logout</a></li>
	      </ul>
	      <div class="user clear">
		<img src="{{asset ('bundles/ssn/images/avatar.jpg') }}" class="avatar" alt="" />
		<span class="user-detail">
		  <span class="name">Welcome </span>
		  <span class="text">Logged as admin</span>
		  <span class="text">You have <a href="/message">messages</a></span>
		</span>
	      </div>
	    </div>
	    <div class="logo clear">
	      <a href="dashboard.php" title="View dashboard">
		<img src="{{asset ('bundles/ssn/images/logo_earth.png') }}" alt="" class="picture" />
		<span class="textlogo">
		  <span class="title">2SN</span>
		  <span class="text">The Super Social Network</span>
		</span>
	      </a>
	    </div>
	  </div>
	</div>
      </div>
      <div class="content-box clear">
	<div class="box-body iconbar">
	  <div class="box-wrap">
	    <div class="main-icons" id="iconbar">
              <ul class="clear">
		<li><a href="{{ path('usersadmin') }}"><img src="{{asset ('bundles/ssn/images/ico_manage-users_48.png') }}" class="icon" alt="" /><span class="text">Users</span></a></li>
		<li><a href="{{ path('publicationsadmin') }}"><img src="{{asset ('bundles/ssn/images/ico_page_64.png') }}" class="icon" alt="" /><span class="text">Publication</span></a></li>
		<li><a href="#"><img src="{{asset ('bundles/ssn/images/ico_picture_64.png') }}" class="icon" alt="" /><span class="text">Media</span></a></li>
		<li><a href="#"><img src="{{asset ('bundles/ssn/images/ico_chat_64.png') }}" class="icon" alt="" /><span class="text">Chat</span></a></li>
		<li><a href="#"><img src="{{asset ('bundles/ssn/images/mylogo.png') }}" class="icon" alt="" /><span class="text">Settings</span></a></li>
		<li><a href="{{path('achievementadmin') }}"><img src="{{asset ('bundles/ssn/images/Achievements.png') }}" class="icon" alt="" /><span class="text">Achievements</span></a></li>
              </ul>
	    </div>
	  </div>
	</div>
      </div>
      {% block body %}{% endblock %}
      {% block javascripts %}
      <script src="{{ asset('bundles/ssn/js/jquery.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.visualize.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.wysiwyg.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.tinymce.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.fancybox.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.idtabs.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.datatables.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.jeditable.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.ui.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.jcarousel.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/jquery.validate.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/excanvas.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/cufon.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/Zurich_Condensed_Lt_Bd.js') }}" type="text/javascript"></script>
      <script src="{{ asset('bundles/ssn/js/script.js') }}" type="text/javascript"></script>
      {% endblock %}
      <div class="login-links">
	<p>
	  <strong>
	    &copy; 2013 Copyright by Random
	  </strong>
	  No rights reserved.
	</p>
      </div>
    </body>
</html>
